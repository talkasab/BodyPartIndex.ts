{
  "name": "@openimagingdata/body_part_index",
  "version": "0.0.3",
  "description": "Provides data and simple libraries for manipulating a standard list of anatomic locations.",
  "module": "src/index.ts",
  "type": "module",
  "license": "ISC",
  "scripts": {
    "build:node": "bun build ./src/index.ts --outdir ./dist --target browser",
    "build:browser": "bun build ./src/index.ts --outdir ./dist --target node --minify",
	"build:types": "tsc --project tsconfig.declarations.json",
	"build": "bun run build:node && bun run build:types",
    "lint": "eslint src --ext .ts",
    "test": "bun test",
    "verify:typescript": "tsc --noEmit",
    "verify:eslint": "eslint src --ext .tsx --ext .ts",
    "verify": "bun run verify:typescript && bun run verify:eslint",
    "publish": "bun run build && clean-publish --access public"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^6.4.0",
    "bun-types": "latest",
    "eslint": "^8.49.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-n": "^15.0.0 || ^16.0.0 ",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-promise": "^6.0.0",
    "prettier": "^3.0.3",
    "typescript": "*"
  },
  "peerDependencies": {
    "typescript": "^5.0.0"
  },
  "dependencies": {
  }
}
